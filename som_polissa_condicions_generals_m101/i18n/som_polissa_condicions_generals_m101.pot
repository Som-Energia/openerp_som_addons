"    md_obj = object.pool.get('ir.model.data')\n"
"\n"
"    template_id = md_obj.get_object_reference(\n"
"        object._cr, object._uid,  'som_poweremail_common_templates', "
"'common_template_legal_footer'\n"
"    )[1]\n"
"\n"
