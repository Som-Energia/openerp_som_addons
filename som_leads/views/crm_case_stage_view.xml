<?xml version="1.0" ?>
<openerp>
    <data>
        <record model="ir.ui.view" id="som_crm_case_stage_tree">
            <field name="name">som.crm.case.stage.tree</field>
            <field name="model">crm.case.stage</field>
            <field name="inherit_id" ref="crm_configuration.crm_case_stage_tree"/>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <field name="section_id" position="after">
                    <field name="method"/>
                </field>
            </field>
        </record>
        <record model="ir.ui.view" id="som_crm_case_stage_form">
            <field name="name">som.crm.case.stage.form</field>
            <field name="model">crm.case.stage</field>
            <field name="inherit_id" ref="crm_configuration.crm_case_stage_form"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <field name="section_id" position="after">
                    <field name="method"/>
                </field>
            </field>
        </record>
    </data>
</openerp>