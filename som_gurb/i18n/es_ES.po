msgstr "con NIF"

#: rml:som_gurb/report/som_gurb_autoritzacio_representant.mako:32
#: rml:som_gurb/report/som_gurb_consentiment_baixa.mako:39
msgid "i domicili social a"
msgstr "y domicilio social en"

#: rml:som_gurb/report/som_gurb_autoritzacio_representant.mako:32
#: rml:som_gurb/report/som_gurb_consentiment_baixa.mako:39
msgid "com a titular del CUPS"
msgstr "como titular del CUPS"

