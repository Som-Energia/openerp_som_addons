            <field name="view_mode">graph,tree,form</field>
            <field name="limit" eval="0"/>
            <field name="res_model">giscedata.switching</field>
            <field name="domain">
                [("state", "=", "open"), ("proces_id.name", "like", "C"), ("step_id.name", "=", "05")]
