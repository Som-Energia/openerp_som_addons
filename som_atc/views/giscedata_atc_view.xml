<?xml version="1.0" ?>
<openerp>
    <data>
        <record id="giscedata_atc-view" model="ir.ui.view">
            <field name="name">giscedata.atc.switching.form</field>
            <field name="model">giscedata.atc</field>
            <field name="type">form</field>
            <field name="priority" eval="1"/>
            <field name="inherit_id" ref="giscedata_atc.giscedata_atc-view"/>
            <field name="arch" type="xml">
                <field name="state" position="after">
                    <field name="tarifa" invisible="1"/>
                    <field name="tipus_autoconsum" invisible="1"/>
                    <field name="te_generation" invisible="1"/>
                    <field name="pending_state" invisible="1"/>
                    <field name="polissa_active" invisible="1"/>
                </field>
            </field>
        </record>
        <record id="giscedata_atc_tree-view" model="ir.ui.view">
            <field name="name">giscedata.atc.switching.tree</field>
            <field name="model">giscedata.atc</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="giscedata_atc.giscedata_atc_tree-view"/>
            <field name="arch" type="xml">
                <field name="name" position="before">
                    <field name="tarifa" select="2"/>
                    <field name="tipus_autoconsum" select="2"/>
                    <field name="te_generation" select="2"/>
                    <field name="pending_state" select="2"/>
                    <field name="polissa_active" select="2"/>
                </field>
            </field>
        </record>
    </data>
</openerp>