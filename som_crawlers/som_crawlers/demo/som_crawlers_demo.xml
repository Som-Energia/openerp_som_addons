<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data noupdate="0">

    <!-- Exemple de Usuari-->
        <record model="som.crawlers.config" id="demo_configuracio_1">
        <field name="name">Endesa</field>
        <field name="usuari">Admin</field>
        <field name="contrasenya">Admin</field>
        <field name="url_portal">https://www.endesa.com/es</field>
        <field name="date_ultima_modificacio">2022-07-12+00:00:00</field>
        <field name="user_ultima_modificacio" ref="base.user_admin"/>
        </record>
    <!-- Exemple accions planificades-->
        <record model="som.crawlers.task" id="demo_accions_planificades_1">
        <field name="name">Endesa descargar R1</field>
        <field name="data_proxima_execucio">2022-07-16+00:00:00</field>
        <field name="configuracio_id" ref="demo_configuracio_1"/>
        <field name="active">True</field>
        </record>
    <!-- Exemple tasca-->
        <record model="som.crawlers.task.step" id="demo_tasca_1">
        <field name="name">Executar fitxer hola.py</field>
        <field name="sequence">1</field>
        <field name="function">executar_un_fitxer</field>
        <field name="params">{'nom_fitxer':'hola.py'}</field>
        <field name="task_id" ref="demo_accions_planificades_1"/>
        </record>

    </data>
</openerp>

  